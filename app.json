{
  "expo": {
    "name": "opcgym",
    "slug": "opcgym",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "opcgym",
    "userInterfaceStyle": "dark",
    "newArchEnabled": true,
    "backgroundColor": "#141414",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#141414"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.googleauth.opcgym"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon-foreground.png",
        "backgroundImage": "./assets/images/adaptive-icon-background.png"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.googleauth.opcgym"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "@react-native-google-signin/google-signin",
        {
          "iosUrlScheme": "com.googleusercontent.apps.33932882657-d07d1r7oog1litaltcld91uok4s1389a"
        }
      ],
      "expo-secure-store"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "d387bdbe-c183-433f-be91-ac78b6257efd"
      }
    }
  }
}
